<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomGraphView">
        <attr name="axesTextColor" format="color"/>
        <attr name="axesColor" format="color"/>
        <attr name="graphColor" format="color"/>
        <attr name="groundTruthColor" format="color"/>
        <attr name="predictedColor" format="color"/>
        <attr name="unionColor" format="color"/>
        <attr name="axisTextSize" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="CustomCoordinatePlateView">
        <attr name="axesTextColor"/>
        <attr name="axesColor"/>
        <attr name="graphColor"/>
        <attr name="unionColor"/>
        <attr name="axisTextSize"/>
    </declare-styleable>

    <declare-styleable name="CustomBoxView">
        <attr name="groundTruthColor"/>
        <attr name="predictedColor"/>
        <attr name="type" format="enum">
            <enum name="GROUND_TRUTH" value="0"/>
            <enum name="PREDICTED" value="1"/>
        </attr>
    </declare-styleable>

</resources>